{
  "expo": {
    "name": "MobileATM",
    "slug": "mobileatm",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/ATMIcon.png",
    "splash": {
      "image": "./app/assets/atm-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-atm-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "description": "Basic UI implementation of what a mobile ATM could look like",
    "githubUrl": "https://github.com/CatB1794/MobileATM"
  }
}
